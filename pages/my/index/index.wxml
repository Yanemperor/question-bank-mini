<!--pages/my/index/index.wxml-->
<block wx:for="{{ items }}" wx:key="index" wx:for-item="item">
  <view class="cell" bindtap="selected" data-name="{{ item.name }}">
    <view class="item">
      <van-icon name="user-o" />
      <view class="item-text">
        {{ item.name }}
      </view>
    </view>
    <van-icon name="arrow" />
  </view>
  <view class="line"></view>
</block>
