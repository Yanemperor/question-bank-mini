<!--pages/file/list/list.wxml-->
<block wx:for="{{ items }}" wx:key="index" wx:for-item="item">
  <view class="cell" bindtap="selected" data-url="{{ item.url }}">
    <view class="item-text">
        {{ item.name }}
      </view>
    <van-icon name="arrow" />
  </view>
  <view class="line"></view>
</block>